<?xml version="1.0"?>
<!DOCTYPE hook PUBLIC "-//Liferay//DTD Hook 6.0.0//EN" "http://www.liferay.com/dtd/liferay-hook_6_0_0.dtd">

<hook>
	<portal-properties>portal-ext.properties</portal-properties>
	<language-properties>content/Language_es_AS</language-properties>
	<servlet-filter>
		<servlet-filter-name>I18n Servlet</servlet-filter-name>
		<servlet-filter-impl>com.liferay.portal.servlet.I18nServlet</servlet-filter-impl>
	</servlet-filter>
	<servlet-filter-mapping>
		<servlet-filter-name>I18n Servlet</servlet-filter-name>
		<url-pattern>/es_AS/*</url-pattern>
	</servlet-filter-mapping>
</hook> 