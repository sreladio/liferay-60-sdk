package prueba.service;

import com.liferay.portal.kernel.util.ClassLoaderProxy;
import com.liferay.portal.kernel.util.MethodHandler;
import com.liferay.portal.kernel.util.MethodKey;

/**
 * @author Sr. Eladio
 */
public class MemberLocalServiceClp implements MemberLocalService {
	public MemberLocalServiceClp(ClassLoaderProxy classLoaderProxy) {
		_classLoaderProxy = classLoaderProxy;

		_addMemberMethodKey0 = new MethodKey(_classLoaderProxy.getClassName(),
				"addMember", prueba.model.Member.class);

		_createMemberMethodKey1 = new MethodKey(_classLoaderProxy.getClassName(),
				"createMember", long.class);

		_deleteMemberMethodKey2 = new MethodKey(_classLoaderProxy.getClassName(),
				"deleteMember", long.class);

		_deleteMemberMethodKey3 = new MethodKey(_classLoaderProxy.getClassName(),
				"deleteMember", prueba.model.Member.class);

		_dynamicQueryMethodKey4 = new MethodKey(_classLoaderProxy.getClassName(),
				"dynamicQuery",
				com.liferay.portal.kernel.dao.orm.DynamicQuery.class);

		_dynamicQueryMethodKey5 = new MethodKey(_classLoaderProxy.getClassName(),
				"dynamicQuery",
				com.liferay.portal.kernel.dao.orm.DynamicQuery.class,
				int.class, int.class);

		_dynamicQueryMethodKey6 = new MethodKey(_classLoaderProxy.getClassName(),
				"dynamicQuery",
				com.liferay.portal.kernel.dao.orm.DynamicQuery.class,
				int.class, int.class,
				com.liferay.portal.kernel.util.OrderByComparator.class);

		_dynamicQueryCountMethodKey7 = new MethodKey(_classLoaderProxy.getClassName(),
				"dynamicQueryCount",
				com.liferay.portal.kernel.dao.orm.DynamicQuery.class);

		_getMemberMethodKey8 = new MethodKey(_classLoaderProxy.getClassName(),
				"getMember", long.class);

		_getMembersMethodKey9 = new MethodKey(_classLoaderProxy.getClassName(),
				"getMembers", int.class, int.class);

		_getMembersCountMethodKey10 = new MethodKey(_classLoaderProxy.getClassName(),
				"getMembersCount");

		_updateMemberMethodKey11 = new MethodKey(_classLoaderProxy.getClassName(),
				"updateMember", prueba.model.Member.class);

		_updateMemberMethodKey12 = new MethodKey(_classLoaderProxy.getClassName(),
				"updateMember", prueba.model.Member.class, boolean.class);
	}

	public prueba.model.Member addMember(prueba.model.Member member)
		throws com.liferay.portal.kernel.exception.SystemException {
		Object returnObj = null;

		MethodHandler methodHandler = new MethodHandler(_addMemberMethodKey0,
				member);

		try {
			returnObj = _classLoaderProxy.invoke(methodHandler);
		}
		catch (Throwable t) {
			if (t instanceof com.liferay.portal.kernel.exception.SystemException) {
				throw (com.liferay.portal.kernel.exception.SystemException)t;
			}

			if (t instanceof RuntimeException) {
				throw (RuntimeException)t;
			}
			else {
				throw new RuntimeException(t.getClass().getName() +
					" is not a valid exception");
			}
		}

		return (prueba.model.Member)ClpSerializer.translateOutput(returnObj);
	}

	public prueba.model.Member createMember(long memberId) {
		Object returnObj = null;

		MethodHandler methodHandler = new MethodHandler(_createMemberMethodKey1,
				memberId);

		try {
			returnObj = _classLoaderProxy.invoke(methodHandler);
		}
		catch (Throwable t) {
			if (t instanceof RuntimeException) {
				throw (RuntimeException)t;
			}
			else {
				throw new RuntimeException(t.getClass().getName() +
					" is not a valid exception");
			}
		}

		return (prueba.model.Member)ClpSerializer.translateOutput(returnObj);
	}

	public void deleteMember(long memberId)
		throws com.liferay.portal.kernel.exception.PortalException,
			com.liferay.portal.kernel.exception.SystemException {
		MethodHandler methodHandler = new MethodHandler(_deleteMemberMethodKey2,
				memberId);

		try {
			_classLoaderProxy.invoke(methodHandler);
		}
		catch (Throwable t) {
			if (t instanceof com.liferay.portal.kernel.exception.PortalException) {
				throw (com.liferay.portal.kernel.exception.PortalException)t;
			}

			if (t instanceof com.liferay.portal.kernel.exception.SystemException) {
				throw (com.liferay.portal.kernel.exception.SystemException)t;
			}

			if (t instanceof RuntimeException) {
				throw (RuntimeException)t;
			}
			else {
				throw new RuntimeException(t.getClass().getName() +
					" is not a valid exception");
			}
		}
	}

	public void deleteMember(prueba.model.Member member)
		throws com.liferay.portal.kernel.exception.SystemException {
		MethodHandler methodHandler = new MethodHandler(_deleteMemberMethodKey3,
				member);

		try {
			_classLoaderProxy.invoke(methodHandler);
		}
		catch (Throwable t) {
			if (t instanceof com.liferay.portal.kernel.exception.SystemException) {
				throw (com.liferay.portal.kernel.exception.SystemException)t;
			}

			if (t instanceof RuntimeException) {
				throw (RuntimeException)t;
			}
			else {
				throw new RuntimeException(t.getClass().getName() +
					" is not a valid exception");
			}
		}
	}

