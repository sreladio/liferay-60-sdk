	public ClassLoaderProxy getClassLoaderProxy() {
		return _classLoaderProxy;
	}

	private ClassLoaderProxy _classLoaderProxy;
	private MethodKey _addMemberMethodKey0;
	private MethodKey _createMemberMethodKey1;
	private MethodKey _deleteMemberMethodKey2;
	private MethodKey _deleteMemberMethodKey3;
	private MethodKey _dynamicQueryMethodKey4;
	private MethodKey _dynamicQueryMethodKey5;
	private MethodKey _dynamicQueryMethodKey6;
	private MethodKey _dynamicQueryCountMethodKey7;
	private MethodKey _getMemberMethodKey8;
	private MethodKey _getMembersMethodKey9;
	private MethodKey _getMembersCountMethodKey10;
