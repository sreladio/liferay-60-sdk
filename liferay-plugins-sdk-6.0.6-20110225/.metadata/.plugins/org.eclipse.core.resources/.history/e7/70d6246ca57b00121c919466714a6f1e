	public static MemberPersistence getPersistence() {
		if (_persistence == null) {
			_persistence = (MemberPersistence)PortletBeanLocatorUtil.locate(com.z.registration.service.ClpSerializer.SERVLET_CONTEXT_NAME,
					MemberPersistence.class.getName());
		}

		return _persistence;
	}

	public void setPersistence(MemberPersistence persistence) {
		_persistence = persistence;
	}

