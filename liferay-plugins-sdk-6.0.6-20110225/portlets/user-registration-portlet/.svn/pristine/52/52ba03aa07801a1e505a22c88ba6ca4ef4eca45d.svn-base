<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" default-destroy-method="destroy" default-init-method="afterPropertiesSet" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="com.oldschool.user.registration.service.USMemberLocalService" class="com.oldschool.user.registration.service.impl.USMemberLocalServiceImpl" />
	<bean id="com.oldschool.user.registration.service.USMemberLocalServiceUtil" class="com.oldschool.user.registration.service.USMemberLocalServiceUtil">
		<property name="service" ref="com.oldschool.user.registration.service.USMemberLocalService" />
	</bean>
	<bean id="com.oldschool.user.registration.service.persistence.USMemberPersistence" class="com.oldschool.user.registration.service.persistence.USMemberPersistenceImpl" parent="basePersistence" />
	<bean id="com.oldschool.user.registration.service.USPhoneNumberLocalService" class="com.oldschool.user.registration.service.impl.USPhoneNumberLocalServiceImpl" />
	<bean id="com.oldschool.user.registration.service.USPhoneNumberLocalServiceUtil" class="com.oldschool.user.registration.service.USPhoneNumberLocalServiceUtil">
		<property name="service" ref="com.oldschool.user.registration.service.USPhoneNumberLocalService" />
	</bean>
	<bean id="com.oldschool.user.registration.service.persistence.USPhoneNumberPersistence" class="com.oldschool.user.registration.service.persistence.USPhoneNumberPersistenceImpl" parent="basePersistence" />
</beans>